package book;

public class fourQuestion {

	int[] a = new int[]{1,2,3,4};
	
	public void temp(int i, int y) {
		int temp;
		temp = i;
		i = y;
		y = temp;
	}
	
	public static void main(String[] args) {
		fourQuestion fq = new fourQuestion();
		int len = fq.a.length;
		
		for(int i = 0; i < (int)(len / 2); i++) {
			fq.temp(fq.a[i], fq.a[len - i - 1]);
		}
		
		System.out.print(a);
	}

}
